{
  "name": "meshblu-connector-powermate",
  "version": "2.0.11",
  "description": "Meshblu Connector ",
  "homepage": "https://github.com/octoblu/meshblu-connector-powermate",
  "bugs": "https://github.com/octoblu/meshblu-connector-powermate/issues",
  "author": {
    "name": "Octoblu",
    "url": "https://github.com/octoblu"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/octoblu/meshblu-connector-powermate.git"
  },
  "scripts": {
    "build": "./build.sh",
    "start": "meshblu-connector",
    "test": "mocha",
    "generate:package": "meshblu-connector-packager",
    "generate:schema": "meshblu-connector-schema-generator > schemas.json",
    "coverage": "nyc npm test",
    "mocha:json": "env NPM_ENV=test mocha --reporter json > coverage/mocha.json",
    "test:watch": "mocha -w -R mocha-multi --reporter-options spec=-,mocha-osx-reporter=-"
  },
  "bin": "command.js",
  "pkg": {
    "targets": [
      "node7-macos-x64",
      "node7-linux-x64",
      "node7-linux-x86",
      "node7-win-x64",
      "node7-win-x86"
    ],
    "scripts": [
      "index.js",
      "src/*.js",
      "jobs/**/*.js",
      "configs/**/*.js"
    ],
    "assets": [
      "package.json",
      "jobs/**/*.json",
      "configs/**/*.json",
      "bin/*.node"
    ]
  },
  "devDependencies": {
    "chai": "^4.0.0",
    "coffee-coverage": "^2.0.1",
    "mocha": "^3.4.2",
    "mocha-multi": "^0.11.0",
    "mocha-osx-reporter": "^0.1.2",
    "nyc": "^10.3.2",
    "pkg": "^3.0.5",
    "segfault-handler": "^1.0.0",
    "sinon": "^2.3.2",
    "sinon-chai": "^2.10.0"
  },
  "dependencies": {
    "async": "^2.4.1",
    "coffee-script": "^1.12.6",
    "debug": "^2.6.8",
    "fs-cson": "^0.3.2",
    "lodash": "^4.17.4",
    "meshblu-config": "^7.0.0",
    "meshblu-connector-cli": "^1.0.3",
    "meshblu-connector-schema-generator": "^2.1.0",
    "node-hid": "^0.5.4"
  },
  "meshbluConnector": {
    "githubSlug": "octoblu/meshblu-connector-powermate"
  },
  "platformDependencies": {
    "darwin": [],
    "win32": [],
    "win64": [],
    "linux": [],
    "android": null,
    "ios": null
  },
  "license": "MIT",
  "keywords": [
    "meshblu-connector",
    "gateblu"
  ],
  "nyc": {
    "cache": true,
    "reporter": [
      "text-summary",
      "lcov",
      "json"
    ],
    "extension": [
      ".coffee"
    ]
  }
}
