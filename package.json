{
  "name": "meshblu-connector-powermate",
  "version": "3.0.10",
  "description": "Meshblu Connector Powermate",
  "homepage": "https://github.com/octoblu/meshblu-connector-powermate",
  "bugs": "https://github.com/octoblu/meshblu-connector-powermate/issues",
  "author": {
    "name": "Octoblu",
    "url": "https://github.com/octoblu"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/octoblu/meshblu-connector-powermate.git"
  },
  "scripts": {
    "build": "coffee --compile --map --output lib/ src/*.coffee",
    "start": "meshblu-connector",
    "test": "mocha",
    "coverage": "nyc npm test",
    "mocha:json": "env NPM_ENV=test mocha --reporter json > coverage/mocha.json",
    "test:watch": "mocha -w -R mocha-multi --reporter-options spec=-,mocha-osx-reporter=-"
  },
  "bin": "command.js",
  "devDependencies": {
    "chai": "^4.0.2",
    "coffee-coverage": "^2.0.1",
    "coffee-script": "^1.12.6",
    "mocha": "^3.4.2",
    "mocha-multi": "^0.11.0",
    "mocha-osx-reporter": "^0.1.2",
    "nyc": "^11.0.2",
    "sinon": "^2.3.4",
    "sinon-chai": "^2.11.0"
  },
  "dependencies": {
    "async": "^2.4.1",
    "debug": "^2.6.8",
    "lodash": "^4.17.4",
    "meshblu-config": "^8.0.1",
    "meshblu-connector-cli": "^1.0.10",
    "node-hid": "^0.5.4",
    "path-exists": "^3.0.0"
  },
  "meshbluConnector": {
    "githubSlug": "octoblu/meshblu-connector-powermate"
  },
  "platformDependencies": {
    "darwin": [],
    "win32": [],
    "win64": [],
    "linux": [],
    "android": null,
    "ios": null
  },
  "license": "MIT",
  "keywords": [
    "meshblu-connector",
    "gateblu"
  ],
  "nyc": {
    "cache": true,
    "reporter": [
      "text-summary",
      "lcov",
      "json"
    ],
    "extension": [
      ".coffee"
    ]
  },
  "pkg": {
    "scripts": [
      "package.json",
      "index.js",
      "src/*.js",
      "jobs/**/*.js",
      "configs/**/*.js"
    ]
  }
}
